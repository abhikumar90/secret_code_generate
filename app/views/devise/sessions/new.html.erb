<h2>Log in</h2>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
   <fieldset>
  <div class="form_data">
     <label for="email">Email:</label>
    <%= f.email_field :email, autofocus: true ,:class=>"email_field",:id=>"email_id"%>
  </div>

  <div class="form_data">
     <label for="password">Password:</label>
    <%= f.password_field :password, autocomplete: "off",:id=>"pass_id" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="form_data">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="button">
    <%= f.submit "Log in" %>
  </div>
   </fieldset>
<% end %>

<%= render "devise/shared/links" %>